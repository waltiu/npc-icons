{
  "name": "npc-icons",
  "version": "0.0.1",
  "description": "图标库",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "scripts": {
    "generate": "tsx scripts/generate.ts",
    "package": "npm run generate && npm run build:dist && npm run build:types",
    "build:dist": "tsx scripts/build.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./global": {
      "types": "./dist/types/global.d.ts",
      "require": "./dist/global.cjs",
      "import": "./dist/global.js"
    },
    "./*": "./*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@pnpm/find-workspace-dir": "^7.0.2",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@types/node": "^22.7.5",
    "camelcase": "^8.0.0",
    "chalk": "^5.3.0",
    "consola": "^3.2.3",
    "esbuild": "^0.24.0",
    "esbuild-plugin-globals": "^0.2.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "potrace": "^2.1.8",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "unplugin-vue": "^5.1.5",
    "vue": "^3.5.10",
    "vue-tsc": "^2.1.6"
  }
}
